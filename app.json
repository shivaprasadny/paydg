{
  "expo": {
    "name": "PayDg",
    "slug": "paydg",
    "scheme": "paydg",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#0B0F1A"
    },
    "ios": {
      "bundleIdentifier": "com.shivaprasad.paydg",
      "supportsTablet": true
    },
    "android": {
      "package": "com.shivaprasad.paydg",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/icon.png",
        "backgroundColor": "#0B0F1A"
      }
    },
    "plugins": ["expo-router"]
  }
}
