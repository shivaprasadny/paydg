{
  "expo": {
    "name": "PayDg",
    "slug": "paydg",
    "scheme": "paydg",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#0B0F1A"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.shivaprasadnyc.paydg"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/icon.png",
        "backgroundColor": "#0B0F1A"
      },
      "package": "com.shivaprasadnyc.paydg"
    },
    "plugins": [
      "expo-router",
      "@react-native-community/datetimepicker",
      "expo-localization",
      "expo-asset",
      "expo-sqlite"
    ],
    "extra": {
      "router": {},
      "eas": {
        "projectId": "e1759fdf-6df5-4b05-8ac2-b5e8cfafe486"
      }
    }
  }
}
